<section id="recent-blog">
  <div class="row top-offer">
    <div class="container">
      <div class="section-title text-center">
        <h2>Últimas Propiedades</h2>
        <h4>Publicadas</h4>
        <h4 class="boton "><%= link_to 'VER MÁS', apartments_path, class: "btn btn-info "%></h4>

      </div>

      <div id="owl-demo">
      <%stop=0%>
      <%Apartment.publicados_sinArrrendar.ultimos.each do |a|%>
        <div class="room-grid-view wow slideInUp">
          <%if a.pictures.empty? %>
          <div class="crop">
            <%= image_tag("sinfoto.jpg", :alt => "No tiene foto", class:"img-responsive") %>
</div>
        <%else%>
<div class="crop">
            <%= image_tag a.pictures.first.image.url, class:"img-responsive" %>
</div>
        <%end%>
          <div class="room-info">
            <div class="post-title">
              <h5><%=a.title%></h5>
              <p><i class="fa fa-calendar"></i> Fecha publicación: <%=a.created_at%></p>
            </div>
            <div class="hotel-ammeties">
                                  <% if a.wifi == true %>
                                    <i class="fa fa-wifi" style="color:orange" title="Wifi"></i>
                                    <%else%>
                                    <i class="fa fa-wifi" title=" Sin Wifi"></i>
                                  <%end%>
                                  <% if a.tv == true %>
                                    <i class="fa fa-television" style="color:orange" title="Television"></i>
                                    <%else%>
                                    <i class="fa fa-television" title=" Sin Television"></i>
                                  <%end%>
                                  <% if a.radio == true %>
                                    <i class="fa fa-headphones" style="color:orange" title="Radio"></i>
                                    <%else%>
                                    <i class="fa fa-headphones" title=" Sin Radio"></i>
                                  <%end%>
                                  <% if a.tvcable == true %>
                                    <i class="fa fa-rss-square" style="color:orange" title="Televisión Satelital"></i>
                                    <%else%>
                                    <i class="fa fa-rss-square" title=" Sin Televisión Satelital"></i>
                                  <%end%>
                                  <% if a.parking == true %>
                                    <i class="fa fa-car" style="color:orange" title="Estacionamiento"></i>
                                    <%else%>
                                    <i class="fa fa-car" title=" Sin Estacionamiento"></i>
                                  <%end%>
                                </div>
            <div class="post-desc">
              <p><%=truncate(a.description,length:45)%></p>
            </div>
            <div class="room-book">
              <div class="col-md-8 col-sm-6 col-xs-6 clear-padding post-alt">
                <h5><%= a.visits_count%> visitas</h5>
              </div>
              <div class="col-md-4 col-sm-6 col-xs-6 clear-padding">
                <%=link_to "Ver", a%>
              </div>
            </div>
            <div class="clearfix"></div>
          </div>
        </div>
        <%break if stop > 5%>
        <%stop += 1%>
        <%end%>
        
      </div>   <!--FIN OWL-->
<script>
    $(document).ready(function() {
      $("#owl-demo").owlCarousel({
          autoPlay: 3000, //Set AutoPlay to 3 seconds
          loop: true,
          magin: 10,
          dots: false,
          navText: [
                    "<i class='fa fa-arrow-left'></i>",
                    "<i class='fa fa-arrow-right'></i>"
                ],
          responsiveClass:false,
          responsive:{
              0:{
                  items:1,
                  nav:true
              },
              600:{
                  items:3,
                  nav:false
              },
              1000:{
                  items:3,
                  nav:true,
                  loop:false
              }
          }
      });
    });
</script> 

